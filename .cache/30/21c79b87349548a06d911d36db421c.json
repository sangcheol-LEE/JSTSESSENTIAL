   ajax.open(\"GET\",url,false);\n   ajax.send();\n\n   return JSON.parse(ajax.response)\n}\n\nconst NEWS_FEED = getData(NEWS_URL);\n\n\n\nconst detailFeed = () => {\n   const locate = location?.hash?.substring(1);\n   const ret = getData(CONTENT_URL.replace(\"@hash\",locate));\n\n   ROOT.innerHTML = `\n      <h1>${ret.title}</h1>\n      <div>\n         <a href=\"#\">목록으로</a>\n      </div>\n   `\n}\n\nconst totalFeed = () => {\n   const newsList = [];\n   newsList.push(\"<ul>\");\n   for(let i = 0; i < NEWS_FEED.length; i++) {\n      newsList.push(`\n         <li>\n            <a href=\"#${NEWS_FEED[i].id}\">\n               ${NEWS_FEED[i].title} (${NEWS_FEED[i].comments_count})\n            </a>\n         </li>\n      `)\n   }\n   newsList.push(\"</ul>\");\n   const ret = newsList.join();\n   ROOT.innerHTML = ret\n}\n\nconst router = () => {\n   const routePath = location.hash;\n   if(routePath === \"\") {\n      totalFeed();\n   }else {\n      detailFeed();\n   }\n}\nwindow.addEventListener(\"hashchange\", router)\nrouter();\n\n"},"lineCount":null}},"error":null,"hash":"2d32adce0652148779a36c7af0c0f616","cacheData":{"env":{}}}